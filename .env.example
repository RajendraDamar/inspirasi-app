# Copy this file to .env and fill with your values (do NOT commit secrets)
# Use EXPO_PUBLIC_ prefixed vars for values that must be available to web builds.

# Firebase (required)
EXPO_FIREBASE_API_KEY=your_api_key_here
EXPO_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_FIREBASE_PROJECT_ID=your_project_id
EXPO_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
EXPO_FIREBASE_APP_ID=your_app_id

# Optional public variants (useful for web builds)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# BMKG API (if you have an API key)
BMKG_API_KEY=

# Any other environment-specific flags
NODE_ENV=development
