{
  "name": "inspirasi-app",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "capture:ui": "node .github/playwright/capture-ui-screens.js",
    "eas:dev": "eas build --profile development --platform all",
    "eas:build": "eas build --profile production --platform all",
    "build:web": "expo build:web --no-pwa --output-dir web-build || expo export:web --output ./web-build"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.3.1",
    "@tanstack/react-query": "^5.0.0",
    "expo": "~53.0.23",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "~13.1.6",
    "firebase": "^12.3.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "5.8.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "react-native-linear-gradient": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "firebase-admin": "^11.12.0",
    "minimist": "^1.2.8",
    "@playwright/test": "^1.40.0"
  }
}
