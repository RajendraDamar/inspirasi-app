{
  "compilerOptions": {
    "target": "ES2020",
    "module": "Node16",
    "jsx": "react",
    "moduleResolution": "node16",
  "allowJs": true,
  "esModuleInterop": true,
  "strict": false,
  "noImplicitAny": true,
  "strictNullChecks": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": [
      "ES2020",
      "DOM"
    ]
  },
  "include": [
    "src",
    "App.tsx",
    "index.ts",
    "index.tsx",
    "app"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
